<?php

/**
 * @file
 * This is the Quicker Edit module for initializing Quick Edit regions faster.
 */

/**
 * Implements hook_page_attachments().
 */
function quicker_edit_page_attachments(array &$page) {
  if (!empty($page['#attached']['library']) && in_array('quickedit/quickedit', $page['#attached']['library'])) {
    $page['#attached']['library'][] = 'quicker_edit/quicker_edit';
  }
}
